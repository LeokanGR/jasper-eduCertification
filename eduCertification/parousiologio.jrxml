	<parameter name="edu_sem_date_inner_param" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select e.`name` as onoma, e.surname, e.phone_num, e.email, DATE_FORMAT(sd.date, "%d/%m/%Y") AS sem_date, p.protocol, e.department, s.title
from edu_employee as e
INNER JOIN edu_participants as p on p.employee_id = e.id
inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
inner join edu_seminar as s on s.id = sd.sem_title_id
where sd.id = $P{edu_sem_date_inner_param}]]>
	</queryString>
