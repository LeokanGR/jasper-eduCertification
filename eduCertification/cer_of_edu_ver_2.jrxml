	<parameter name="edu_sem_date_param" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select 
	(select e.`name`
	from edu_employee as e
	INNER JOIN edu_participants as p on p.employee_id = e.id
	inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
	inner join edu_seminar as s on s.id = sd.sem_title_id
	where sd.id = $P{edu_sem_date_param}
	and p.officer_flag = 1) as onoma_ekpaideftis,
	(select e.surname
	from edu_employee as e
	INNER JOIN edu_participants as p on p.employee_id = e.id
	inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
	inner join edu_seminar as s on s.id = sd.sem_title_id
	where sd.id = $P{edu_sem_date_param}
	and p.officer_flag = 1) as surname_ekpaideftis, 
	(select e.phone_num
	from edu_employee as e
	INNER JOIN edu_participants as p on p.employee_id = e.id
	inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
	inner join edu_seminar as s on s.id = sd.sem_title_id
	where sd.id = $P{edu_sem_date_param}
	and p.officer_flag = 1) as phone_num_ekpaideftis, 
	(select e.email
	from edu_employee as e
	INNER JOIN edu_participants as p on p.employee_id = e.id
	inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
	inner join edu_seminar as s on s.id = sd.sem_title_id
	where sd.id = $P{edu_sem_date_param}
	and p.officer_flag = 1) as email_ekpaideftis, 
	(select sd.date
	from edu_employee as e
	INNER JOIN edu_participants as p on p.employee_id = e.id
	inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
	inner join edu_seminar as s on s.id = sd.sem_title_id
	where sd.id = $P{edu_sem_date_param}
	and p.officer_flag = 1) AS sem_date_ekpaideftis, 
	e.`name` as onoma,
	e.surname,
	e.phone_num,
	e.email,
	sd.date AS sem_date, 
	sd.date_end AS sem_date_end, 
	p.protocol,
	e.department,
	s.title
from edu_employee as e
INNER JOIN edu_participants as p on p.employee_id = e.id
inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
inner join edu_seminar as s on s.id = sd.sem_title_id
where sd.id = $P{edu_sem_date_param}  AND p.attendance = true]]>
	</queryString>
