		<queryString language="SQL">
			<![CDATA[select date_format(sd.date, "%d/%m/%Y") as sem_date, p.protocol, e.surname, e.`name` as onoma, e.department, s.title
from edu_employee as e
INNER JOIN edu_participants as p on p.employee_id = e.id
inner JOIN edu_seminar_dates as sd on sd.id = p.sem_date_id
inner join edu_seminar as s on s.id = sd.sem_title_id
where sd.id = $P{inner_param}]]>
